<%= search_form_for @search do |f| %>
  <%= f.search_field :username_cont, class: "search-box", placeholder: " ユーザー検索" %>
  <%= f.submit "検索", class: "search-btn" %>
<% end %>
<% if @search_users.presence %>
  <% @search_users.each do |user| %>
    <ul class="search_users"><%= link_to user.username, user_path(user.id) %></ul>
  <% end %>
  <%= paginate @search_users, :param_name => 'user_page' %>
<% end %>
